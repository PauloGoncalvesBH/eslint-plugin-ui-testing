{
  "name": "eslint-plugin-ui-testing",
  "version": "0.0.0",
  "description": "ESLint rules for UI testing frameworks WebdriverIO, Cypress, TestCafe, Playwright, Puppeteer",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Kwo Ding",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kwoding/eslint-plugin-ui-testing.git"
  },
  "homepage": "https://github.com/kwoding/eslint-plugin-ui-testing",
  "bugs": {
    "url": "https://github.com/kwoding/eslint-plugin-ui-testing/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && npm run lint && tsc",
    "postbuild": "cpy package.json ./dist && cpy README.md ./dist && cpy LICENSE ./dist",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --clearCache && jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^7",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0",
    "npm": ">=6"
  }
}
